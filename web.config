<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
			<rules>
				<rule name="Force NonHTTPS" stopProcessing="true">
					<match url="(.*)" />
					<conditions>
					   <add input="{HTTPS}" pattern="on" />
					</conditions>
					<action type="Redirect" url="http://{HTTP_HOST}/{REQUEST_URI}" />
				</rule>
			</rules>
		</rewrite>
    </system.webServer>
</configuration>
